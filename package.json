{
  "name": "ynab-tui",
  "version": "0.4.0",
  "description": "Interactive TUI for YNAB with AI-powered transaction categorization",
  "type": "module",
  "bin": {
    "ynab-tui": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/cli.tsx",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "ynab",
    "budget",
    "budgeting",
    "finance",
    "tui",
    "cli",
    "terminal",
    "ai",
    "categorization",
    "openrouter",
    "ink",
    "react"
  ],
  "author": "Abdallah <contact@abdallahaho.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbdallahAHO/ynab-tui.git"
  },
  "bugs": {
    "url": "https://github.com/AbdallahAHO/ynab-tui/issues"
  },
  "homepage": "https://github.com/AbdallahAHO/ynab-tui#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "@inkjs/ui": "^2.0.0",
    "@openrouter/ai-sdk-provider": "^0.4.3",
    "ai": "^4.1.0",
    "commander": "^14.0.2",
    "ink": "^5.1.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "jotai": "^2.11.0",
    "react": "^18.3.1",
    "ynab": "^2.10.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "husky": "^9.1.7",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  }
}
